{
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://your-backend-domain.onrender.com",
    "VITE_SOCKET_URL": "https://your-backend-domain.onrender.com"
  }
}